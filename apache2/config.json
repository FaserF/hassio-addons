  {
    "name": "Apache2",
    "version": "1.2.2",
    "slug": "apache2",
    "description": "Apache2 Webserver Addon ",
    "url": "https://github.com/FaserF/hassio-addons/tree/master/apache2",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "application",
    "boot": "auto",
    "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
    "map": ["share:rw", "ssl"],
    "ingress": true,
    "panel_icon": "mdi:web-box",
    "options": {
      "document_root": "/share/htdocs",
      "website_name":null,
      "ssl": true,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem"
    },
    "schema": {
      "document_root":"str",
      "website_name":"str",
      "ssl": "bool",
      "certfile": "str",
      "keyfile": "str"
    },
    "ports": {
      "80/tcp": 80,
      "443": 443
    },
    "ports_description": {
      "80/tcp": "Web interface http",
      "443/tcp": "SSL Web interface"
    }
  }
